{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/dashboard",
      "dest": "/api/dashboard.js"
    },
    {
      "src": "/download",
      "dest": "/api/download.js"
    }
  ]
}
